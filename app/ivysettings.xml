<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <properties file="build.properties"/>
    <credentials host="http://localhost:8082/" 
				 realm="Sonatype Nexus Repository Manager" 
				 username="admin" 
				 passwd="admin123"/>
    <settings defaultResolver="public"/>
	<resolvers>
		<ibiblio name="public" m2compatible="true" useMavenMetadata="false" root="http://localhost:8082/content/repositories/php/"/>
		<url name="publish" m2compatible="true">
		    <artifact pattern="http://localhost:8082/nexus/content/repositories/releases/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]"/>
		</url>
	</resolvers>
	<modules/>
</ivysettings>